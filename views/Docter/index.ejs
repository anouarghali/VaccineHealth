                              <div class="iq-card-body">
                                 <div class="iq-progress-bar progress-bar-vertical iq-bg-primary">
                                       <span class="bg-primary" data-percent="70"></span>
                                   </div>
                                 <span class="line-height-4"> <%- docter.completedVaccinations.length -  docter.patients.length %> </span>
                                 <h4 class="mb-2 mt-2">Enrolled Children</h4>
                                 <p class="mb-0 text-secondary line-height">*******</p>
                              </div>
                           </div>
                        </div>
                        <div class="col-md-6 col-lg-4">
                           <div class="iq-card iq-card-block iq-card-stretch iq-card-height">
                              <div class="iq-card-body">
                                 <div class="iq-progress-bar progress-bar-vertical iq-bg-danger">
                                    <% if(  docter.patients.length == 0 ){ %>
                                       <% var p2 = 0; %>
                                    <% }else { %>
                                       <%   var p2 =80 %>
                                    <%  } %>
                                       <span class="bg-danger" data-percent="<%=p2%>"></span>
                                   </div>
                                 <span class="line-height-4"> <%= docter.patients.length %> </span>
                                 <h4 class="mb-2 mt-2">Total Vaccines Given</h4>
                                 <p class="mb-0 text-secondary line-height">*******</p>
                              </div>
                           </div>
                        </div>
