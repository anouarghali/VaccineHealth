                                    <% if( docter.patients.length == 0 ){ %>
                                       <% var p3 = 0; %>
                                    <% }else { %>
                                       <%   var p3 = docter.completedVaccinations.length / docter.patients.length * 100 %>
                                   <%  } %>
                                   <span class="bg-warning" data-percent="<%=p3%>"></span>
                                   </div>
                                 <span class="line-height-4"> <%= docter.completedVaccinations.length %> </span>
                                 <h4 class="mb-2 mt-2">Total Vaccines Completed</h4>
                                 <p class="mb-0 text-secondary line-height">*******</p>
                              </div>
                           </div>
                        </div>
